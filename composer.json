{
    "name": "lukehowland/helpdeskwidget",
    "description": "Widget embebible para integrar Helpdesk en proyectos Laravel externos",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas De La Quintana Montenegro",
            "email": "lukqs05@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lukehowland\\HelpdeskWidget\\": "src/"
        }
